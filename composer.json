{
	"name": "code-area/laraarea-api",
	"description": "api crud with login",
	"keywords": ["laravel", "api"],
	"license": "MIT",
	"authors": [
		{
			"name": "Codearea"
		}
	],
	"require": {
		"laravel/framework": "6.*",
		"code-area/laraarea-validator": "9.0.*",
		"code-area/laraarea-transformer": "9.0.*",
		"code-area/laraarea-model": "9.0.*",
		"lcobucci/jwt": "3.3.*"
	},
	"require-dev": {
		"mockery/mockery": "^1.0",
		"phpunit/phpunit": "^8.0"
	},
	"extra": {
		"laravel": {
			"providers": [
				"LaraAreaApi\\Providers\\ServiceProvider"
			]
		}
	},
	"autoload": {
		"psr-4": {
			"LaraAreaApi\\": "src/"
		}
	},
	"suggest": {
		"code-area/laraarea-helpers": "Useful functions"
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	}
}
